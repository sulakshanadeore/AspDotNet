<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="MainPage.master.cs" Inherits="MasterPagesDemo.MainPage" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>Paytm</title>
    <style type="text/css">
        .auto-style1 {
            width: 100%;
            border-left-style: solid;
            border-left-width: 1px;
            border-right: 1px solid #C0C0C0;
            border-top-style: solid;
            border-top-width: 1px;
            border-bottom: 1px solid #C0C0C0;
        }
        .auto-style2 {
            height: 23px;
        }
        .auto-style3 {
            height: 23px;
